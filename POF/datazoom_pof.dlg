	BUTTON btn_2008				20 +42 120 25, label("2008") onpush(script child_2008)

	BUTTON btn_2017				20 +42 120 25, label("2017") onpush(script child_2017)
	
	FRAME fr_1995   			0  0 400 45

	FRAME fr_2002	  			0  +43 400 45

	FRAME fr_2008   			0  +43 400 45

	FRAME fr_2017   			0  +43 400 45
