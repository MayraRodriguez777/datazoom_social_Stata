RESET res1

SCRIPT PREINIT_SCRIPT
   BEGIN
      create STRING nomeDialog
      nomeDialog.storeDialogClassName
   END

SCRIPT child_1995
   BEGIN
      create CHILD datazoom_pof1995
   END   
 
SCRIPT child_2002
   BEGIN
      create CHILD datazoom_pof2002
   END   

SCRIPT child_2008
   BEGIN
      create CHILD datazoom_pof2008
   END   

